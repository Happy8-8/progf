<div class="booyah-box col-10 offset-1">
  <h1><%= @food.name %></h1>
  <br/><br/>
  <p><%= @food.description %></p>
  <div class="float-right">
    <%= link_to 'Edit', edit_food_path(@food), class: 'btn btn-primary' %>
    <%= link_to 'Destroy', food_path(@food), method: :delete, data: {confirm: 'Are you sure you want to delete this?'},class: 'btn btn-danger' %>
  </div>
</div>